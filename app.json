{
  "expo": {
    "name": "Warranty Tracker",
    "owner": "warranty",
    "slug": "warranty-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ef4f5a1f-5f84-4be1-9379-334d914d3f71"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.warrantytracker.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ef4f5a1f-5f84-4be1-9379-334d914d3f71"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Add photos of your items to make filing claims easier.",
          "cameraPermission": "Take photos of your items to make filing claims easier."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
